		"eslint": "^8.47.0",
		"eslint-config-prettier": "^9.0.0",
		"npm-check-updates": "^16.11.1",
		"npm-run-all": "^4.1.5",
		"prettier": "^3.0.2",
		"rimraf": "^5.0.1",
		"serve": "^14.2.0",
		"typescript": "^5.1.6",
		"vitest": "^0.34.1"
	},
	"dependencies": {
		"cac": "^6.7.14",
		"glob": "^10.3.3",
