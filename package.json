	"private": true,
	"license": "MIT",
	"scripts": {
		"build": "esbuild src/index.ts --bundle --platform=node --outfile=dist/index.js --packages=external --minify",
		"watch": "esbuild src/index.ts --bundle --platform=node --outfile=dist/index.js --packages=external --watch",
		"lint": "eslint src",
